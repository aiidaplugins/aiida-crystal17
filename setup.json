{
    "name": "aiida-crystal17",
    "author": "Chris Sewell",
    "author_email": "chrisj_sewell@hotmail.com",
    "description": "AiiDA plugin for running the CRYSTAL17 code",
    "url": "https://github.com/chrisjsewell/aiida-crystal17",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Programming Language :: Python :: 2.7",
        "Topic :: Scientific/Engineering :: Chemistry",
        "Topic :: Scientific/Engineering :: Physics"
    ],
    "entry_points": {
        "aiida.calculations": [
            "crystal17.multiply = aiida_crystal17.calculations.multiply:MultiplyCalculation"
        ],
        "aiida.parsers": [
            "crystal17.product = aiida_crystal17.parsers.multiply:MultiplyParser"
        ],
        "aiida.data": [
            "crystal17.factors = aiida_crystal17.data.multiply:MultiplyParameters"
        ],
        "aiida.cmdline.data": [
            "crystal17.factors = aiida_crystal17.data.data_cli:cli"
        ]
    },
    "scripts": ["examples/cli.py"],
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida >= 0.12.0"
    ],
    "extras_require": {
        "testing": [
            "aiida-core[testing]"
        ],
        "pre-commit": [
            "pre-commit",
            "yapf",
            "prospector",
            "pylint"
        ],
        "docs": [
            "sphinx"
        ]
    }
}
