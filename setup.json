{
    "name": "aiida-crystal17",
    "author": "Rico Haeuselmann, Leopold Talirz",
    "author_email": "haeuselm@epfl.ch",
    "description": "AiiDA plugin template for testing and getting started with development",
    "url": "https://github.com/aiidateam/aiida-crystal17",
    "license": "Creative Commons",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "version": "0.2.0",
    "entry_points": {
        "aiida.calculations": [
            "crystal17.multiply = aiida_crystal17.calculations.multiply:MultiplyCalculation"
        ],
        "aiida.parsers": [
            "crystal17.product = aiida_crystal17.parsers.multiply:MultiplyParser"
        ],
        "aiida.data": [
            "crystal17.factors = aiida_crystal17.data.multiply:MultiplyParameters"
        ],
        "aiida.cmdline.data": [
            "crystal17.factors = aiida_crystal17.data.data_cli:cli"
        ]
    },
    "scripts": ["examples/cli.py"],
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida >= 0.11.0"
    ],
    "extras_require": {
        "testing": [
            "aiida-core[testing]"
        ],
        "pre-commit": [
            "pre-commit",
            "yapf",
            "prospector",
            "pylint"
        ],
        "docs": [
            "sphinx"
        ]
    }
}
